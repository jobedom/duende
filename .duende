{
    "title": "Duende",
    "apps": {
        "editor": "Sublime Text.app",
        "ide": "Visual Studio Code.app",
        "terminal": "iTerm.app"
    }, 
    "options": [{
            "kind": "application",
            "label": "Editar .duende",
            "cmd": "%editor %p/.duende"
        },
        "-", {
            "kind": "application",
            "label": "Terminal",
            "cmd": "%terminal %p"
        },
        {
            "kind": "application",
            "label": "Visual Studio Code",
            "cmd": "%ide %p",
            "auto": true
        },
        "-",
        {
            "kind": "command",
            "label": "Build",
            "cmd": "yarn build",
            "auto": true
        },
        {
            "kind": "command",
            "label": "Watch",
            "cmd": "yarn watch",
            "auto": true
        }
    ]
}
